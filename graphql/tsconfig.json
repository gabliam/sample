{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["es2015", "dom"],
    "types": [
      "reflect-metadata",
      "express"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": false,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "sourceMap": false,
    "outDir": "build"
  }
}
